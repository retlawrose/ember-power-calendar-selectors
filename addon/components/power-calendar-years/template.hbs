{{#let  
  (concat "power-calendar-years/" attrs.calendar.type) 
  (hash
    calendar=attrs.calendar
    disabledDates=attrs.disabledDates
    format=format
    maxDate=attrs.maxDate
    maxLength=attrs.maxLength
    minDate=attrs.minDate
    onSelectYear=attrs.onSelectYear
    proximitySelection=attrs.proximitySelection
  )
  as |componentName publicAPI|}}
  {{#if hasBlock}}
    {{#component componentName publicAPI=publicAPI as |year calendar|}}
      {{yield year calendar}}
    {{/component}}
  {{else}}
    {{component componentName publicAPI=publicAPI}}
  {{/if}}
{{/let}}