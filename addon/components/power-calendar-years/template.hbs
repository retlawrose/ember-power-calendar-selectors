{{#let  
  (concat "power-calendar-years/" @calendar.type) 
  (hash
    calendar=@calendar
    disabledDates=@disabledDates
    format=format
    maxDate=@maxDate
    maxLength=@maxLength
    minDate=@minDate
    onSelectYear=@onSelectYear
    proximitySelection=@proximitySelection
  )
  as |componentName publicAPI|}}
  {{#if hasBlock}}
    {{#component componentName publicAPI=publicAPI as |year calendar|}}
      {{yield year calendar}}
    {{/component}}
  {{else}}
    {{component componentName publicAPI=publicAPI}}
  {{/if}}
{{/let}}