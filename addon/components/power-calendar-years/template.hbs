{{#if hasBlock}}
  {{#component (concat "power-calendar-years/" type)
    calendar=@calendar
    disabledDates=@disabledDates
    format=@format
    maxDate=@maxDate
    minDate=@minDate
    onSelect=@onSelect
    onSelectYear=@onSelectYear
    as |year calendar|
  }}
    {{yield year calendar}}
  {{/component}}
{{else}}
  {{component (concat "power-calendar-years/" type)
    calendar=@calendar
    disabledDates=@disabledDates
    format=@format
    maxDate=@maxDate
    minDate=@minDate
    onSelect=@onSelect
    onSelectYear=@onSelectYear
  }}
{{/if}}
